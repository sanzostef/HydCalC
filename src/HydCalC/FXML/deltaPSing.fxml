<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="130.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HydCalC.DeltaPSingController">

    <!-- TextField Perte de charge -->
    <TextField id="diam" layoutX="145.0" onKeyTyped="#modif" layoutY="5.0" prefHeight="25.0" prefWidth="50.0" />
    <TextField id="masseVol" onKeyTyped="#modif" layoutX="145.0" layoutY="35.0" prefHeight="25.0" prefWidth="50.0" />
    <ComboBox id="incident" fx:id="cbIncident" onAction="#choixIncident" layoutX="145.0" layoutY="65.0" prefWidth="150.0" />
    <TextField id="DeltaP" layoutX="145.0" layoutY="95.0" prefHeight="25.0" prefWidth="50.0" />
    <!-- Label DeltaP -->
    <Label layoutX="5.0" layoutY="10.0" text="Diametre (Ø):" />
    <Label layoutX="5.0" layoutY="40.0" text="Masse Volume (Densité):" />
    <Label layoutX="5.0" layoutY="70.0" text="Type d'incident (K):" />
    <Label layoutX="5.0" layoutY="100.0" text="Pertes de charge (ΔP):" />

</Pane>
